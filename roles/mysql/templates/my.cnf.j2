[mysqld]
basedir                 = /usr/local/mysql
datadir                 = /data/mysql
port                    = 3306
user                    = mysql

[mysql]
prompt                  = (\\u@\\h) [\\d]>\\_
skip_auto_rehash

[client]
protocol                = TCP
host                    = 127.0.0.1
user                    = root
password                = root